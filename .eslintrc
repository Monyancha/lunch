{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": 0,
    "key-spacing": 0,
    "no-confusing-arrow": 0,
    "jsx-quotes": [2, "prefer-double"],
    "max-len": [2, 120],
    "react/jsx-no-bind": [2, {"ignoreRefs": true}],
    "no-param-reassign": 0
  }
}
