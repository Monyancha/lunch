.form-summary-data
  %span= t('letters_of_credit.beneficiary_new.received', datetime: fhlb_datetime_long_alpha_with_on(@created_at))
  %br
  %br
  %span= t('letters_of_credit.beneficiary_new.contact')
  %br
  %span= t('letters_of_credit.beneficiary_new.questions_html', phone_number: service_desk_phone_number, rm_email: @contacts[:rm][:email], rm_name: @contacts[:rm][:full_name], rm_phone_number: fhlb_formatted_phone_number(@contacts[:rm][:phone_number]))
  %br
  %br
  %br
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary_new.member_name'))
    %dd= @member_name_email
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary_new.fhfa_number'))
    %dd= @fhfa
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary_new.requested_by'))
    %dd= @requested_by
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.name'))
    %dd= @beneficiary_request.name
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.care_of') + ' ' + t('global.optional_field'))
    %dd= @beneficiary_request.care_of.blank? ? t('global.missing_value') : @beneficiary_request.care_of
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.department') + ' ' + t('global.optional_field'))
    %dd= @beneficiary_request.department.blank? ? t('global.missing_value') : @beneficiary_request.department
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.street_address'))
    %dd= @beneficiary_request.street_address
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.city'))
    %dd= @beneficiary_request.city
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.state'))
    %dd= @beneficiary_request.state
  %dl
    %dt= t('global.defined_term', term: t('letters_of_credit.beneficiary.zip'))
    %dd= @beneficiary_request.zip
