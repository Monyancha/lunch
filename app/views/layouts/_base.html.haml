- header_template ||= 'layouts/header'
!!!
%html
  %head
    %meta{charset: "utf-8"}
    %meta{content: "IE=edge,chrome=1", :'http-equiv' => "X-UA-Compatible"}
    %title= @page_title || 'FHLB Member Portal'
    = stylesheet_link_tag "application", media: "all"
    = javascript_include_tag  "application"
    = csrf_meta_tags
  %body
    %header.page-header= render header_template
    %main
      = yield